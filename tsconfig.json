{
  "compilerOptions": {
    "allowJs": true,
    "experimentalDecorators": true,
    "declaration": false,
    "module": "esNext",
    "target": "es2015",
    "jsx": "preserve",
    "jsxFactory": "createElement",
    "moduleResolution": "node",
    "alwaysStrict": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "suppressImplicitAnyIndexErrors": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "react": ["node_modules/types/react"],
      "@eva/eva.js": ["node_modules/@eva/eva.js/types"],
      "@eva/eva-plugin-renderer": ["node_modules/@eva/eva-plugin-renderer/types"],
      "@eva/eva-plugin-renderer-render": ["node_modules/@eva/eva-plugin-renderer-render/types"],
      "@eva/eva-plugin-renderer-text": ["node_modules/@eva/eva-plugin-renderer-text/types"],
      "@eva/eva-plugin-renderer-event": ["node_modules/@eva/eva-plugin-renderer-event/types"],
      "@eva/eva-plugin-a11y": ["node_modules/@eva/eva-plugin-renderer-a11y/types"],
      "@eva/eva-plugin-renderer-dragonbone": ["node_modules/@eva/eva-plugin-renderer-dragonbone/types"],
      "@eva/eva-plugin-renderer-graphics": ["node_modules/@eva/eva-plugin-renderer-graphics/types"],
      "@eva/eva-plugin-renderer-img": ["node_modules/@eva/eva-plugin-renderer-img/types"],
      "@eva/eva-plugin-renderer-spine": ["node_modules/@eva/eva-plugin-renderer-spine/types"],
      "@eva/eva-plugin-renderer-sprite": ["node_modules/@eva/eva-plugin-renderer-sprite/types"],
      "@eva/eva-plugin-renderer-sprite-animation": ["node_modules/@eva/eva-plugin-renderer-sprite-animation/types"],
      "@eva/eva-plugin-renderer-tiling-sprite": ["node_modules/@eva/eva-plugin-renderer-tiling-sprite/types"],
      "@eva/eva-plugin-transition": ["node_modules/@eva/eva-plugin-transition/types"],
      // "@eva/driver-eva": ["packages/driver-eva/types"],
      "@eva/reconciler-eva": ["packages/reconciler-eva/types"],
      "@eva/rax-eva": ["packages/rax-eva/types"],
      "@eva/rax-eva-a11y": ["packages/rax-eva-a11y/types"],
      "@eva/rax-eva-dragonbone": ["packages/rax-eva-dragonbone/types"],
      "@eva/rax-eva-graphics": ["packages/rax-eva-graphics/types"],
      "@eva/rax-eva-image": ["packages/rax-eva-image/types"],
      "@eva/rax-eva-mask": ["packages/rax-eva-mask/types"],
      "@eva/rax-eva-spine": ["packages/rax-eva-spine/types"],
      "@eva/rax-eva-sprite": ["packages/rax-eva-sprite/types"],
      "@eva/rax-eva-sprte-animation": ["packages/rax-eva-sprte-animation/types"],
      "@eva/rax-eva-tiling-sprite": ["packages/rax-eva-tiling-sprite/types"],
      "@eva/rax-eva-transition": ["packages/rax-eva-transition/types"]
    }
  }
}
