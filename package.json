    "@eva/plugin-renderer-event": "^1.2.1",
    "@eva/plugin-renderer-graphics": "^1.2.1",
    "@eva/plugin-renderer-img": "^1.2.1",
    "@eva/plugin-renderer-lottie": "^1.2.1",
    "@eva/plugin-renderer-mask": "^1.2.1",
    "@eva/plugin-renderer-nine-patch": "^1.2.1",
    "@eva/plugin-renderer-render": "^1.2.1",
    "@eva/plugin-renderer-spine": "^1.2.1",
    "@eva/plugin-renderer-sprite": "^1.2.1",
    "@eva/plugin-renderer-sprite-animation": "^1.2.1",
    "@eva/plugin-renderer-text": "^1.2.1",
