{
  "name": "react-eva",
  "version": "0.1.1",
  "author": "Chrise",
  "private": true,
  "engines": {
    "npm": ">=10.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "clean": "lerna clean && rm -rf node_modules packages/**/es packages/**/dist packages/**/lib",
    "start": "node scripts/start.js",
    "update": "node scripts/update.js",
    "build": "node scripts/build.js",
    "init": "npm run bootstrap && npm run build -- --packages=*",
    "reset": "npm run clean && npm run init",
    "publish": "node scripts/publish.js",
    "disttag": "node scripts/disttag.js",
    "playground-dev": "cd packages/playground && npm run dev",
    "playground-public": "cd packages/playground && npm run public"
  },
  "dependencies": {
    "@eva/eva.js": "^1.2.1",
